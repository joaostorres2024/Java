package Main;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
    	Scanner valorTotal = new Scanner(System.in);
    	double valorTotalCompra = valorTotal.nextDouble();
    	
    	Scanner valorRecebido = new Scanner(System.in);
    	double valorRecebidoLeitura = valorRecebido.nextDouble();
    	
    	double calculoTroco = valorRecebidoLeitura - valorTotalCompra;
    	
    	if(calculoTroco < 0) {
    		System.out.println("R$ 0.00");
    	}else {
    		System.out.printf("%.2f%n", calculoTroco);
    	}
    	
    }
} 
